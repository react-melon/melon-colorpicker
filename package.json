{
    "name": "melon-colorpicker",
    "version": "1.0.3",
    "description": "Melon Color Picker",
    "main": "lib/index.js",
    "scripts": {
        "start": "node tools/server.js",
        "build": "gulp build",
        "lint": "fecs src",
        "rebuild": "gulp clean build",
        "prepublish": "npm run lint && npm run rebuild"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/react-melon/melon-colorpicker.git"
    },
    "keywords": [
        "react",
        "melon",
        "color",
        "picker",
        "color-picker"
    ],
    "author": "leon <ludafa@outlook.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/react-melon/melon-colorpicker/issues"
    },
    "homepage": "https://github.com/react-melon/melon-colorpicker#readme",
    "dependencies": {
        "melon": "^0.3.4",
        "melon-core": "^0.3.2"
    },
    "devDependencies": {
        "babel-loader": "^6.2.5",
        "babel-plugin-external-helpers": "^6.8.0",
        "babel-plugin-external-helpers-insert-require": "^1.0.0",
        "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
        "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
        "babel-plugin-transform-es3-property-literals": "^6.8.0",
        "babel-plugin-transform-object-rest-spread": "^6.8.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.11.1",
        "css-loader": "^0.24.0",
        "fecs": "^0.8.6",
        "file-loader": "^0.9.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^6.1.2",
        "gulp-babel-external-helpers": "^2.0.2",
        "gulp-clean": "^0.3.2",
        "nib": "^1.1.2",
        "style-loader": "^0.13.1",
        "stylus": "^0.54.5",
        "stylus-loader": "^2.3.1",
        "webpack": "^1.13.2",
        "webpack-dev-server": "^1.15.0",
        "webpack-hot-middleware": "^2.12.2"
    },
    "babel": {
        "presets": [
            "es2015",
            "react"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    },
    "babelBuild": {
        "presets": [
            [
                "es2015",
                {
                    "loose": true,
                    "modules": "umd"
                }
            ],
            "react"
        ],
        "plugins": [
            "transform-object-rest-spread",
            "external-helpers",
            "external-helpers-insert-require",
            "transform-es3-property-literals",
            "transform-es3-member-expression-literals"
        ]
    }
}
